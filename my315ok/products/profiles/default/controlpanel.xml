<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="my315ok.products">

    <configlet
        title="My315okProducts settings"
        action_id="my315okprocuts"
        appId="my315okprocuts"
